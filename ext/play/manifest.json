{
  "manifest_version": 3,
  "name": "Play Extension",
  "version": "1.0",
  "permissions": ["activeTab", "webNavigation", "tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
